      "email": "fanweixiao@gmail.com"
  ],
  "devDependencies": {
    "@types/node-fetch": "^2.5.12",
    "@types/react": "^18.0.9",
    "husky": "^7.0.4",
    "node-fetch": "^2.6.6",
    "react": "^18.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "tsup": "^6.5.0",
    "typescript": "^4.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yomorun/webtransport-polyfill.git"
  },
