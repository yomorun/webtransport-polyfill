{
  "name": "@yomo/webtransport-polyfill",
  "version": "1.0.3",
  "description": "Polyfill the WebTransport protocol using WebSocket fallback if the browser does not support it natively.",
  "keywords": [
    "webtransport",
    "websocket"
  ],
  "license": "MIT",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "tsup ./src/index.ts --dts --watch",
    "build": "tsup ./src/index.ts --dts"
  },
  "np": {
    "yarn": false,
    "contents": "dist"
  },
  "author": "yomo",
  "contributors": [
